{
  "lambdaLayers": [
    {
      "type": "ProjectLayer",
      "resourceName": "bigboibigboilambdalayer",
      "version": "Always choose latest version",
      "isLatestVersionSelected": true,
      "env": "dev"
    }
  ],
  "permissions": {
    "api": {
      "bigboiapi": [
        "create",
        "read",
        "update",
        "delete"
      ]
    }
  },
  "environmentVariableList": [
    {
      "cloudFormationParameterName": "blogDeployToken",
      "environmentVariableName": "BLOG_DEPLOY_TOKEN"
    },
    {
      "cloudFormationParameterName": "blogDeployWebhook",
      "environmentVariableName": "BLOG_DEPLOY_WEBHOOK"
    },
    {
      "cloudFormationParameterName": "blogDeployProjectId",
      "environmentVariableName": "BLOG_DEPLOY_PROJECT_ID"
    },
    {
      "cloudFormationParameterName": "blogDeployBranch",
      "environmentVariableName": "BLOG_DEPLOY_BRANCH"
    }
  ]
}
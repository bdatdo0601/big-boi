{
  "permissions": {
    "api": {
      "bigboiapi": [
        "create",
        "read",
        "update",
        "delete"
      ]
    }
  },
  "lambdaLayers": [
    {
      "type": "ProjectLayer",
      "resourceName": "bigboibigboilambdalayer",
